<?xml version="1.0" encoding="utf-8"?>
<com.example.administrator.nothing_kotlin.cardview.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_marginTop="@dimen/x15"
    android:layout_marginBottom="@dimen/x15"
    android:layout_marginLeft="@dimen/x10"
    android:layout_marginRight="@dimen/x10"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:cardCornerRadius="@dimen/x5">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <ImageView
            android:id="@+id/tab_img"
            android:scaleType="centerCrop"
            android:layout_width="match_parent"
            android:layout_height="@dimen/x280" />
        <TextView
            android:id="@+id/tab_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/x10"
            android:layout_marginBottom="@dimen/x10"
            android:layout_marginLeft="@dimen/x10"
            android:layout_marginRight="@dimen/x10"
            android:textSize="@dimen/x28"
            android:maxLines="1"
            android:ellipsize="end"
            android:textColor="@color/text_color_black"/>
    </LinearLayout>
</com.example.administrator.nothing_kotlin.cardview.CardView>